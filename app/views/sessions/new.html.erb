<%= form_tag sessions_path, :name => "associate_login_frm", :id => "associate_login_frm", :method => "post" do %>
<p>
<%= text_field_tag "user_id", "", :placeholder => "Associate ID", :class => "required"%>
<span id="username_warning" style="color:red"></span> 
<%= password_field_tag "password", "", :placeholder => "Password", :class => "required" %>
<span id="password_warning" style="color:red"></span>
</p>

<p> <%= check_box_tag "agree" %> Agree to pull my information from Cognizant system <p>

<p> <%= submit_tag "Let me USE!", :type => "submit" %> <%= submit_tag "I am not READY!", :type => "reset" %> <p>

<% end %>


<script type="text/javascript">
$(document).ready(function () {               
    form_id = $('form').attr('id');
    $("#"+form_id).validate();
});
</script>